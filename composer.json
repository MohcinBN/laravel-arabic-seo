{
    "name": "mohcin/laravel-arabic-seo",
    "description": "A Laravel package for Arabic SEO optimization, including meta tags and slug generation",
    "type": "library",
    "keywords": ["laravel", "seo", "arabic", "slug", "meta-tags", "rtl"],
    "homepage": "https://github.com/MohcinBN/laravel-arabic-seo",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohcin",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "ArabicSeo\\Seo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
          "providers": [
            "ArabicSeo\\Seo\\SeoServiceProvider"
          ],
          "aliases": {
            "Seo": "ArabicSeo\\Seo\\Facades\\Seo"
          }
        }
      },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "minimum-stability": "stable"
}
